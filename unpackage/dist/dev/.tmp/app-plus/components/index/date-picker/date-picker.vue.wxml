<template name="17d20915"><view style="width:100%" class="_view data-v-a8fd8e40"><view class="_view data-v-a8fd8e40 order-time" bindtap="handleProxy" data-eventid="{{'17d20915-0'}}" data-comkey="{{$k}}"><view class="_view data-v-a8fd8e40 box"><text class="_text data-v-a8fd8e40 iconfont goInHotel"></text><text class="_text data-v-a8fd8e40 date-wrappper">{{ choiceDate[0].month }}月{{ choiceDate[0].day }}日</text><text class="_text data-v-a8fd8e40 goInHotel2" wx:if="{{choiceDate[0].year +''+ choiceDate[0].month +''+  choiceDate[0].day == today}}">今天</text><text class="_text data-v-a8fd8e40 left-hotel">-</text><text class="_text data-v-a8fd8e40 date-wrappper">{{ choiceDate[1].month }}月{{ choiceDate[1].day }}日</text><text class="_text data-v-a8fd8e40 goInHotel2" wx:if="{{choiceDate[choiceDate.length - 1].year +''+  choiceDate[choiceDate.length - 1].month +''+  choiceDate[choiceDate.length - 1].day == tomorrow}}">明天</text></view><text class="_text data-v-a8fd8e40 sumCount">{{ dayCount2 }}</text></view><view class="_view data-v-a8fd8e40 calendar-layer {{isShow ? 'show' : 'hide'}}" animation="{{animationData}}"><view class="_view data-v-a8fd8e40 layer-content choiceDate"><text class="_text data-v-a8fd8e40 iconfont shanchu" bindtap="handleProxy" data-eventid="{{'17d20915-1'}}" data-comkey="{{$k}}"></text><view class="_view data-v-a8fd8e40 layer-header"><view class="_view data-v-a8fd8e40 layer-close" bindtap="handleProxy" data-eventid="{{'17d20915-2'}}" data-comkey="{{$k}}"></view><text class="_text data-v-a8fd8e40 layer-title">选择日期</text></view><view class="_view data-v-a8fd8e40 layer-body"><view class="_view data-v-a8fd8e40 week-box"><text class="_text data-v-a8fd8e40 week-box-item" wx:key="tmpIndex" key="{{tmpIndex}}" wx:for="{{weekNameArr}}" wx:for-index="tmpIndex" wx:for-item="item">{{ item }}</text></view><scroll-view class="_scroll-view data-v-a8fd8e40 layer-list" scroll-y="true"><view wx:key="index" key="{{index}}" class="_view data-v-a8fd8e40 month" wx:for="{{date}}" wx:for-index="index" wx:for-item="monthData"><view class="_view data-v-a8fd8e40 month-title" id="{{'m-' + monthData[0].year + '-' + monthData[0].month}}" style=" {{'z-index:' + index}}">{{ monthData[0].year + '年' + monthData[0].month + '月' }}</view><view class="_view data-v-a8fd8e40 month-content"><view wx:key="index2" key="{{index2}}" class="_view data-v-a8fd8e40 day {{data.dateTime < today  ? 'disabled'  : data.selected  ? 'active' +    (data.date == choiceDate[0].date      ? ' begin'      : data.date == choiceDate[1].date      ? ' end'      : '')  : ''}}" data-index="{{index}}" data-indexs="{{index2}}" bindtap="handleProxy" data-eventid="{{'17d20915-3-'+index+'-'+index2}}" data-comkey="{{$k}}" wx:for="{{monthData}}" wx:for-index="index2" wx:for-item="data" style=" {{index2 == 0  ? 'margin-left:' + (weeks[index] == 6 ? 0 : weeks[index]) * (100 / 7) + '%'  : ''}}"><view class="_view data-v-a8fd8e40 day-content"><text class="_text data-v-a8fd8e40 day-subject">{{ data.dateTime != today && data.dateTime != tomorrow && data.dateTime != afterTomorrow ? data.act.subject : '' }}</text><text class="_text data-v-a8fd8e40 day-txt">{{ data.dateTime == today ? '今天' : data.dateTime == tomorrow ? '明天' : data.dateTime == afterTomorrow ? '后天' : data.day }}</text><text class="_text data-v-a8fd8e40 day-tip">{{ data.act.tip }}</text></view><view class="_view data-v-a8fd8e40 beginTip" wx:if="{{choice === false}}">请选择离店日期</view><view class="_view data-v-a8fd8e40 endTip" wx:if="{{choice}}">{{ dayCount2 }}</view></view></view></view></scroll-view></view><view class="_view data-v-a8fd8e40 layer-footer"><view class="_view data-v-a8fd8e40 submitBtn" bindtap="handleProxy" data-eventid="{{'17d20915-4'}}" data-comkey="{{$k}}">完成</view></view></view></view></view></template>